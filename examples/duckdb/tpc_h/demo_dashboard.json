    "gridItems": {
      "0": {
        "type": "markdown",
        "content": "## Manufacturer Overview\n\nFor manufacturers, review sales, products, and regions. Click on a manufacturer to drill down.",
        "name": "Note 0",
        "width": 719,
        "height": 75,
        "conceptFilters": [],
        "filters": []
      },
      "1": {
        "type": "chart",
        "content": "\r\n \r\n# Selecting total revenue\r\n# Total revenue is based on the extended price minus discount\r\nselect \r\n    local.total_revenue;",
        "name": "Chart 1",
        "width": 173,
        "height": 75,
        "chartConfig": {
          "chartType": "headline",
          "xField": "total_revenue",
          "yField": "",
          "yField2": "",
          "yAggregation": "sum",
          "colorField": "",
          "sizeField": "",
          "groupField": "",
          "trellisField": "",
          "geoField": "",
          "showDebug": false
        },
        "filters": [],
        "chartFilters": [],
        "conceptFilters": []
      },
      "2": {
        "type": "chart",
        "content": "  \n# This query calculates the total items sold by counting the local.id of each sold item.  \n# The total sold items are derived simply by counting the ids, which is a boolean indicator of whether an item has sold.  \nselect sum(quantity) as total_items_sold  \n;  \n",
        "name": "Chart 2",
        "width": 173,
        "height": 75,
        "conceptFilters": [],
        "filters": []
      },
      "3": {
        "type": "chart",
        "content": "  \n# The user requests the total number of orders.  \n# We will count the order.id field to find out the total orders.  \n\nselect count(order.id) as total_orders  \n;  \n",
        "name": "Chart 3",
        "width": 173,
        "height": 75,
        "conceptFilters": [],
        "filters": []
      },
      "4": {
        "type": "chart",
        "content": " \n# Counting the total number of customers\nselect count(order.customer.id) as total_customers;\n",
        "name": "Chart 4",
        "width": 173,
        "height": 75,
        "conceptFilters": [],
        "filters": []
      },
      "5": {
        "type": "chart",
        "content": "\n# Selecting the best selling products based on total revenue\nselect \n    part.id as product_id, \n    part.name, \n    sum(revenue) as total_revenue \nhaving \n    total_revenue > 0 \norder by \n    total_revenue desc\nlimit 15;\n",
        "name": "Chart 5",
        "width": 355,
        "height": 275,
        "conceptFilters": [],
        "filters": [],
        "chartConfig": {
          "chartType": "barh",
          "xField": "total_revenue",
          "yField": "part_name",
          "yField2": "",
          "yAggregation": "sum",
          "colorField": "part_name",
          "sizeField": "",
          "groupField": "",
          "trellisField": "",
          "geoField": "",
          "showDebug": false
        },
        "chartFilters": []
      },
      "6": {
        "type": "chart",
        "content": " \n# This query selects the count of local.id \n# for each country, representing items sold to each country. \n\nselect \n    order.customer.nation.name, \n    sum(quantity) as items_sold \nhaving \n    items_sold > 0;\n\n",
        "name": "Sales By Country",
        "width": 355,
        "height": 275,
        "conceptFilters": [],
        "filters": [],
        "chartFilters": []
      },
      "7": {
        "type": "chart",
        "content": " \n# Query to get total sales by supplier \n# Total sales is calculated as the extended price minus discount\n\nselect \n    part.manufacturer, \n    total_revenue\nhaving total_revenue > 0 \norder by\n    total_revenue desc\n; \n",
        "name": "Chart 7",
        "width": 719,
        "height": 195,
        "chartFilters": [],
        "chartConfig": {
          "chartType": "bar",
          "xField": "part_manufacturer",
